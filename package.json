	"keywords": [],
	"homepage": "",
	"bugs": "https://github.com/germanfrelo/wp-block-description-list/issues",
	"repository": "github:germanfrelo/wp-block-description-list",
	"license": "GPL-2.0-or-later",
	"author": "Germán Freixinós López",
	"type": "module",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --blocks-manifest --webpack-copy-php",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"make-pot": "wp-scripts i18n make-pot . languages/gfl-block-description-list.pot",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --blocks-manifest --webpack-copy-php",
		"start:hot": "wp-scripts start --blocks-manifest --webpack-copy-php --hot"
